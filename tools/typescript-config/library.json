{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React & React Native Library",
  "extends": "./base.json",
  "compilerOptions": {
    "allowJs": true,
    "jsx": "react-jsx",
    "lib": ["DOM", "ESNext"],
    "noEmit": true,
    "resolveJsonModule": true,
    "module": "node16",
    "target": "ESNext"
  }
}
